<testcase xmlns="tag:dmaus@dmaus.name,2019:Schematron:Testsuite" expect="error" id="let-name-collision-error-03">
  <label>It is an error for a variable to be multiply defined in the current schema</label>
  <reference>ISO Schematron 2016: Section 5.4.5 Clause 3</reference>
  <documents>
    <primary filename="document.xml">
      <element xmlns=""/>
    </primary>
  </documents>
  <schemas>
    <sch:schema xmlns:sch="http://purl.oclc.org/dsdl/schematron">
      <sch:let name="foo" value="'bar'"/>
      <sch:let name="foo" value="'bar'"/>
      <sch:pattern>
        <sch:rule context="/">
          <sch:assert test="true()"/>
        </sch:rule>
      </sch:pattern>
    </sch:schema>
  </schemas>
</testcase>
